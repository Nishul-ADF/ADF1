{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nishul-ADF"
		},
		"SQL_Emp_table_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_Emp_table'"
		},
		"SQL_Emp_table_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "rg-nishul-server.database.windows.net"
		},
		"SQL_Emp_table_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "nishulDB"
		},
		"SQL_Emp_table_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nishul"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nishul-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SQL_Emp_table_password": {
						"value": "[parameters('SQL_Emp_table_password')]"
					},
					"SQL_Emp_table_properties_typeProperties_server": {
						"value": "[parameters('SQL_Emp_table_properties_typeProperties_server')]"
					},
					"SQL_Emp_table_properties_typeProperties_database": {
						"value": "[parameters('SQL_Emp_table_properties_typeProperties_database')]"
					},
					"SQL_Emp_table_properties_typeProperties_userName": {
						"value": "[parameters('SQL_Emp_table_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}